{
  "name": "fuzzbox",
  "version": "1.0.0",
  "description": "Micro chaos engineering and API fuzzing middleware for Express and Next.js. Intentionally break things before your users do.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "docs",
    "README.md",
    "LICENSE",
    "SECURITY.md",
    "CONTRIBUTING.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "node tests/core.test.js && node tests/mutators.test.js && node tests/integration.test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "chaos",
    "chaos-engineering",
    "fuzzing",
    "api-fuzzing",
    "testing",
    "express",
    "nextjs",
    "middleware",
    "resilience",
    "fault-injection"
  ],
  "author": "eneswritescode <eneswrites@protonmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eneswritescode/fuzzbox"
  },
  "homepage": "https://github.com/eneswritescode/fuzzbox#readme",
  "bugs": {
    "url": "https://github.com/eneswritescode/fuzzbox/issues"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "next": "^14.2.35",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
